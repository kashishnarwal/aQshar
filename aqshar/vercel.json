{
  "builds": [{
    "src": "aqshar/wsgi.py",
    "use": "@vercel/python",
    "config": {
      "maxLambdaSize": "15mb",
      "runtime": "python3.9"
    }
  }],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "http://aqshar.vercel.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "http://aqshar.vercel.app/$1"
    }
  ]
}
